/**index.wxss**/
page{
  background-color:#AEEEEE;  
}
.t_red{
  color: red;
}
.t_blue{
  color: royalblue;
}
.calendar{
  margin: 10vw;
}
.time{
  padding: 16rpx 20rpx;
  display: flex;
}
.time view{
  flex: 1;
  font-size: 30rpx;
margin-bottom:10rpx;

}
.time view text{
  font-size: 38rpx;
}
.weekName{
   background-color: #54ff9c;
  width: 100%;
  display: flex;
  padding: 16rpx 0;
}
.weekName view{
  flex: 1;
  text-align: center;
}
.sunday,.saturday{
  color: red;
}
.week{
  width: 100%;
  background-color: white;
}
.weekbottom{
  width: 100%;
  background-color: white;
height: 13vw;
}
.week view{
  width: 14.2%;
  height: 60rpx;
  line-height: 80rpx;
  display: inline-block;
  margin: 20rpx 0;
  text-align: center;
  
}
.week view text{
  width: 100%;
  height: 100%;

  display: inline-block;
}

.dz {
  position: relative;
  top:12rpx;
}

.f {
  position: relative;
  margin-top: 20rpx;
  height: 20vw;
  width: 20vw;
  bottom: 100rpx;
  padding: 0;
  margin: 0 auto;
}

.f button {
  width: 20vw;
  height: 20vwx;
  border: none;
  padding: 0;
}

.f image {
  width: 20vw;
  height: 20vw;
}